#!/bin/bash
merah="\e[31m"
kuning="\e[33m"
hijau="\e[32m"
biru="\e[34m"
UL="\e[4m"
bold="\e[1m"
italic="\e[3m"
reset="\e[m"
echo -e "$bold$italic$hijau"
 echo "                                                                                         :                                                              "
echo "                                               G:              ,;                       t#,                                   ,;        ,;L.             "
echo "                                    j.         E#,    :      f#i                 .Gt   ;##W.           .Gt j.               f#i       f#i EW:        ,ft "
echo "            ..       :           .. EW,        E#t  .GE    .E#t  GEEEEEEEL      j#W:  :#L:WE          j#W: EW,            .E#t      .E#t  E##;       t#E "
echo "           ,W,     .Et          ;W, E##j       E#t j#K;   i#W,   ,;;L#K;;.    ;K#f   .KG  ,#D       ;K#f   E##j          i#W,      i#W,   E###t      t#E "
echo "          t##,    ,W#t         j##, E###D.     E#GK#f    L#D.       t#E     .G#D.    EE    ;#f    .G#D.    E###D.       L#D.      L#D.    E#fE#f     t#E "
echo "         L###,   j###t        G###, E#jG#W;    E##D.   :K#Wfff;     t#E    j#K;     f#.     t#i  j#K;      E#jG#W;    :K#Wfff;  :K#Wfff;  E#t D#G    t#E "
echo "       .E#j##,  G#fE#t      :E####, E#t t##f   E##Wi   i##WLLLLt    t#E  ,K#f   ,GD;:#G     GK ,K#f   ,GD; E#t t##f   i##WLLLLt i##WLLLLt E#t  f#E.  t#E "
echo "      ;WW; ##,:K#i E#t     ;W#DG##, E#t  :K#E: E#jL#D:  .E#L        t#E   j#Wi   E#t ;#L   LW.  j#Wi   E#t E#t  :K#E:  .E#L      .E#L     E#t   t#K: t#E "
echo "     j#E.  ##f#W,  E#t    j###DW##, E#KDDDD###iE#t ,K#j   f#E:      t#E    .G#D: E#t  t#f f#:    .G#D: E#t E#KDDDD###i   f#E:      f#E:   E#t    ;#W,t#E "
echo "   .D#L    ###K:   E#t   G##i,,G##, E#f,t#Wi,,,E#t   jD    ,WW;     t#E      ,K#fK#t   f#D#;       ,K#fK#t E#f,t#Wi,,,    ,WW;      ,WW;  E#t     :K#D#E "
echo "  :K#t     ##D.    E#t :K#K:   L##, E#t  ;#W:  j#t          .D#;    t#E        j###t    G#t          j###t E#t  ;#W:       .D#;      .D#; E#t      .E##E "
echo "  ...      #G      .. ;##D.    L##, DWi   ,KK:  ,;            tt     fE         .G#t     t            .G#t DWi   ,KK:        tt        tt ..         G#E "
echo "           j          ,,,      .,,                                    :           ;;                    ;;                                            fE "
echo "                                                                                                                                                       , "
echo -e "$reset$bold$kuning====================>>($biru https://github.com/mggnet $kuning)<<===================$reset\n"
